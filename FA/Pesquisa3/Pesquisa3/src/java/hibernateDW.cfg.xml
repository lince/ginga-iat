    <mapping resource="POJODW/DimUserHasDimSocialnetwork.hbm.xml"/>
    <mapping resource="POJODW/IteractionHasDimUser.hbm.xml"/>
    <mapping resource="POJODW/IteractionHasDimTime.hbm.xml"/>
    <mapping resource="POJODW/Iteraction.hbm.xml"/>
    <mapping resource="POJODW/DimUser.hbm.xml"/>
    <mapping resource="POJODW/IteractionHasDimMedia.hbm.xml"/>
    <mapping resource="POJODW/IteractionHasDimLocation.hbm.xml"/>
    <mapping resource="POJODW/DimMedia.hbm.xml"/>
    <mapping resource="POJODW/IteractionHasDimDevice.hbm.xml"/>
    <mapping resource="POJODW/IteractionHasDimProgram.hbm.xml"/>
    <mapping resource="POJODW/DimTime.hbm.xml"/>
